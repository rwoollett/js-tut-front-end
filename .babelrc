{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-destructuring", { 
      "useBuiltIns": true 
      }
    ],
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-computed-properties",
    "react-hot-loader/babel"
  ]
}